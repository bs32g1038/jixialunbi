{
  "name": "node-bbs-server",
  "version": "1.0.0",
  "description": "A simple and refreshing micro community, developed based on nodejs, typescript, Prisma, ant design, nextjs",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "generate:typings": "ts-node generate-typings.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "generate": "prisma generate --schema ./prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "9.1.6",
    "@nestjs/core": "9.1.6",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "9.1.6",
    "@nestjs/platform-socket.io": "^9.1.6",
    "@nestjs/websockets": "^9.1.6",
    "@prisma/client": "4.5.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/crypto-js": "^4.1.1",
    "@types/express-session": "^1.17.5",
    "@types/find-root": "^1.1.2",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.186",
    "@types/lodash-es": "^4.17.6",
    "@types/multer": "^1.4.7",
    "@types/request-ip": "^0.0.37",
    "canvaskit-wasm": "^0.37.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express-rate-limit": "^6.6.0",
    "express-session": "^1.17.3",
    "find-root": "^1.1.0",
    "joi": "^17.6.4",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "log4js": "^6.7.0",
    "lru-cache": "^7.14.0",
    "meilisearch": "^0.29.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prisma": "^4.5.0",
    "reflect-metadata": "0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "3.0.2",
    "rxjs": "7.5.7",
    "skia-canvas": "^1.0.1",
    "socket.io": "^4.5.3",
    "uid": "^2.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "9.1.5",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.6",
    "@types/express": "4.17.14",
    "@types/node": "18.11.8",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "29.2.2",
    "prettier": "2.7.1",
    "supertest": "6.3.1",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-morph": "16.0.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  }
}
